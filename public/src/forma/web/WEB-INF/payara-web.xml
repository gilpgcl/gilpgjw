<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE payara-web-app PUBLIC
 "-//Payara.fish//DTD Payara Server 4 Servlet 3.0//EN"
 "https://docs.payara.fish/schemas/payara-web-app_4.dtd">
<payara-web-app error-url="">
  <!-- El contenido se crea automÃ¡ticamente. Solo se necesita modificar la parte
  del final. -->
  <class-loader delegate="true"/>
  <jsp-config>
    <property name="keepgenerated" value="true">
      <description>
        Keep a copy of the generated servlet class' java code.
      </description>
    </property>
  </jsp-config>
  <!-- Esto es lo que se agrega para manejar correctamente "UTF-8". -->
  <parameter-encoding default-charset="UTF-8"/>
</payara-web-app>
